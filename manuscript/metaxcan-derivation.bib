issn = {1061-4036},
journal = {Nature Genetics},
keywords = {*Polymorphism, Single Nucleotide,Alternative Splicing/*genetics,Base Sequence,Blood Cells/*metabolism,DNA Mutational Analysis,Disease/genetics,Gene Expression Profiling,Genetic Predisposition to Disease,Genome-Wide Association Study,Humans,Protein Isoforms/*genetics,Quantitative Trait Loci/*genetics,RNA Splice Sites/genetics,Transcriptome},
