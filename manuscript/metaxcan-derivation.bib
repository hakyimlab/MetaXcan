journal = {Lancet},
keywords = {Adult,Age of Onset,Attention Deficit Disorder with Hyperactivity,Attention Deficit Disorder with Hyperactivity: epi,Attention Deficit Disorder with Hyperactivity: gen,Bipolar Disorder,Bipolar Disorder: epidemiology,Bipolar Disorder: genetics,Calcium Channels,Child,Child Development Disorders,Depressive Disorder,Genetic Loci,Genetic Loci: genetics,Genome-Wide Association Study,Humans,L-Type,L-Type: genetics,Logistic Models,Major,Major: epidemiology,Major: genetics,Pervasive,Pervasive: epidemiology,Pervasive: genetics,Polymorphism,Schizophrenia,Schizophrenia: epidemiology,Schizophrenia: genetics,Single Nucleotide,Single Nucleotide: genetics},
number = {9875},
pages = {1371--9},
